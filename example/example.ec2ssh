path '~/.ssh/config'
aws_keys(
  {
    access_key_id: ENV['AWS_ACCESS_KEY_ID'],
    secret_access_key: ENV['AWS_SECRET_ACCESS_KEY']
  },
  #{
  #  access_key_id: '...'
  #  secret_access_key: '...'
  #}
)
regions 'ap-northeast-1'
#skip_if {|instance| instance.tags['Name'] =~ /.../ }
host_lines <<END
Host <%= tags['Name'] %>
  HostName <%= private_ip_address %>
END
